{
  "name": "tiger-server",
  "version": "0.2.1",
  "description": "Even simple than trigger",
  "main": "lib/index.js",
  "repository": "https://github.com/kenpusney/tiger",
  "author": "Kimmy Leo",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.3",
    "log4js": "^3.0.5",
    "node-cron": "^1.2.0",
    "pouchdb": "^7.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node-cron": "^1.2.0",
    "@types/pouchdb": "^6.3.2",
    "chai": "^4.1.2",
    "ignore-styles": "^5.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "files": [
    "lib/**/*.js",
    "src/**/*.ts",
    "types/**/*.ts"
  ],
  "scripts": {
    "test": "nyc --reporter=html mocha -r ts-node/register -r ignore-styles src/**/*.spec.ts"
  },
  "types": "./types/index.d.ts",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "example/",
      "lib/",
      "coverage/",
      "**/*.spec.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
